{
  "name": "gvns",
  "type": "dark",
  "colors": {
    "editor.background": "#0f1114",
    "editor.foreground": "#eceff4",
    "editorLineNumber.foreground": "#6b7280",
    "editorLineNumber.activeForeground": "#a3aab5",
    "editorCursor.foreground": "#4a7c59",
    "editor.selectionBackground": "#252a33",
    "editor.inactiveSelectionBackground": "#1a1d23"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#6b7280", "fontStyle": "italic" }
    },
    {
      "scope": ["string", "punctuation.definition.string"],
      "settings": { "foreground": "#c9a959" }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": { "foreground": "#8fbc8f" }
    },
    {
      "scope": ["entity.name.function", "support.function", "variable.function"],
      "settings": { "foreground": "#5d9a6e" }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": { "foreground": "#81a1c1" }
    },
    {
      "scope": ["entity.name.type", "support.type"],
      "settings": { "foreground": "#8fbc8f" }
    },
    {
      "scope": ["punctuation", "meta.brace", "meta.delimiter"],
      "settings": { "foreground": "#a3aab5" }
    }
  ]
}
